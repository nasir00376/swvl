    "@types/express": "^4.17.9",
    "@types/mongodb": "^3.6.3",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.20",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
